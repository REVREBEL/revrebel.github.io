    "fsevents": "^2.3.2",
    "graphql": "^16.7.1",
    "lerna": "^7.1.1",
    "moment": "^2.29.4",
    "netlify-cms-app": "^2.15.72",
    "netlify-cms-core": "^2.55.2",
    "netlify-cms-proxy-server": "^1.3.24",
    "nx": "^16.5.0",
    "postcss": "^8.4.25",
    "postcss-syntax": "^0.36.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-scripts": "5.0.1",
    "redux": "^4.2.1",
